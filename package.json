{
	"name": "bibliotech-backend",
	"version": "1.0.0",
	"type": "module",
	"description": "Application de gestion dâ€™extraits de code",
	"main": "index.js",
	"scripts": {
		"test": "jest",
		"format": "pnpx @biomejs/biome format ./",
		"format:fix": "pnpx @biomejs/biome format --write ./",
		"lint": "pnpx @biomejs/biome lint ./",
		"lint:fix": "pnpx @biomejs/biome lint --write ./",
		"check": "pnpx @biomejs/biome check ./",
		"check:fix": "pnpx @biomejs/biome check --write ./",
		"dev": "nodemon --watch src --ext ts --exec \"node --loader ts-node/esm src/index.ts\"",
		"create:db": "psql -U bibliotech < data/create-db.sql",
		"seed:db": "psql -U bibliotech < data/seed-db.sql",
		"update:user": "psql -U bibliotech < data/seed-user-db.sql",
		"reset:db": "npm run create:db && npm run seed:db && npm run update:user",
		"build": "tsc",
		"start": "node dist/index.js"
	},
	"keywords": [
		"node",
		"express",
		"typescript",
		"jest",
		"api",
		"code-snippets",
		"developer-tools",
		"backend",
		"rest-api",
		"snippets-manager"
	],
	"author": "Whitney CLAIRE",
	"license": "ISC",
