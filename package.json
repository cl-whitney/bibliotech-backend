{
	"name": "bibliotech-backend",
	"version": "1.0.0",
	"type": "module",
	"description": "Application de gestion dâ€™extraits de code",
	"main": "index.js",
	"scripts": {
		"test": "jest --detectOpenHandles",
		"format": "pnpx @biomejs/biome format ./",
		"format:fix": "pnpx @biomejs/biome format --write ./",
		"lint": "pnpx @biomejs/biome lint ./src",
		"lint:fix": "pnpm exec biome lint ./src --fix",
		"check": "pnpx @biomejs/biome check ./",
		"check:fix": "pnpx @biomejs/biome check --write ./",
		"dev": "nodemon --watch src --ext ts --exec tsx src/index.ts",
		"create:db": "psql -U bibliotech < data/01-create-db.sql",
		"seed:db": "psql -U bibliotech < data/03-seed-db.sql",
		"seed:user": "psql -U bibliotech < data/02-seed-user-db.sql",
		"reset:db": "pnpm run create:db && pnpm run seed:user && pnpm run seed:db",
		"build": "tsc",
		"start": "node dist/index.js"
	},
	"keywords": [
		"node",
		"express",
		"typescript",
		"jest",
		"api",
